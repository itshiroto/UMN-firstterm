@article{albertusKonsumerismeTerhadapGawai2020,
  title = {{Konsumerisme terhadap Gawai dan Dampaknya bagi Lingkungan: Studi Kasus pada Apple Inc.}},
  shorttitle = {{Konsumerisme terhadap Gawai dan Dampaknya bagi Lingkungan}},
  author = {Albertus, Yanuar},
