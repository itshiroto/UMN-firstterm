@article{schmalzriedInnerBeautyFriendshipHypothesis2013,
  title = {Inner {{Beauty}} \textemdash{} {{The Friendship-Hypothesis}}},
  author = {Schmalzried, Lisa Katharin},
  year = {2013},
  volume = {5},
  pages = {23},
  abstract = {This article analyses different hypotheses about inner beauty. In a Platonic tradition, an inwardly beautiful person is considered to be a moral person. But not all moral actions and character traits are equally relevant for inner beauty. And inner beauty also depends on morally praiseworthy actions and character traits. Furthermore, morally neutral character traits like intelligence, humour, or optimism matter as well. This paper formulates and defends the friendship-hypothesis: an inwardly beautiful person behaves towards others as if they were her friends, and her personality and her character traits would make her a good friend. The friendship-hypothesis alludes to the traditionally assumed close connection between beauty and love. And it can explain why also judgements of inner beauty flicker between objectivity and subjectivity. As the friendship-hypothesis only indicates similarities between inner beauty and beauty in general, it is compatible both with a literal and a metaphorical interpretation of inner beauty.},
  langid = {english},
  file = {E\:\\kuliah\\zotero\\storage\\54XFPGCS\\Schmalzried - 2013 - Inner Beauty â€” The Friendship-Hypothesis.pdf}
}

