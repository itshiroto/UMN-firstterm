@article{retnowatiPUSARANPEMILIHANKEPALA2015,
  title = {{PUSARAN PEMILIHAN KEPALA DAERAH SECARA LANGSUNG ATAU TIDAK LANGSUNG}},
  author = {Retnowati, Endang},
  year = {2015},
  month = jan,
  journal = {Perspektif},
